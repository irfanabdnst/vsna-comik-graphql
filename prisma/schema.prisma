generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type Bookmark {
  id   String? @default(cuid()) @map("_id") @db.ObjectId
  name String?
}

type Profile {
  firstName String?
  lastName  String?
}

model user {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId
  email     String
  username  String
  password  String
  profile   Profile?
  bookmarks Bookmark[]
}
